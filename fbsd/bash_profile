#PS1='\u\033[0;94m@\e[0m\h \033[0;94m<\W>\033[0m\033[0;36m\$\033[0m '
COLOR_CLOSE='\[\033[0m'
USER='\u'
HOST='\h'
DIR='\w'
DOL='\$'
COLOR_RED='\[\033[31m'
PS1="$COLOR_RED[$COLOR_CLOSE$USER$COLOR_RED@$COLOR_CLOSE$HOST$COLOR_RED]$COLOR_CLOSE:$COLOR_RED<$COLOR_CLOSE$DIR$COLOR_RED>$DOL$COLOR_CLOSE "
#PS1='\033[31m[\033[0m\u\033[31m
